{
    "name": "dvsa/mot-web-frontend",
    "require": {
        "zendframework/zendframework": "2.2.7",
        "doctrine/common": "v2.4.1",
        "dvsa/transition-group": "dev-master",
        "dvsa/feature-toggle": "1.0",
        "dvsa/dvsa-document-module": "dev-develop",
        "dvsa/dvsa-logger": "v1.0.0",
        "dvsa/scripts": "dev-master",
        "dvsa/dvsa-logger": "v1.0.2",
        "dvsa/dvsa-openam-client" : "v1.5.0",
        "maglnet/magl-markdown": "1.3.0",
        "soflomo/purifier": "0.1.1",
        "slot-purchase/web-frontend": "2.3.*",
        "dvsa/web-frontend-assets": "0.2.10",
        "dvsa/dvsa-application-logger": "v1.0.0"
    },
    "autoload" : {
        "psr-0" : {
            "DvsaCommon" : "../mot-common-web-module/src",
            "DvsaCommonTest" : "../mot-common-web-module/test"
        },
        "psr-4" : {
            "Dvsa\\Mot\\Frontend\\": "src",
            "Dvsa\\Mot\\FrontendTest\\": "test/src"
        },
        "classmap" : [
            "../mot-common-web-module/Module.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "cd .. && ln -fs mot-web-frontend/vendor/dvsa/scripts/jenkins Jenkins_Scripts",
            "mkdir -p public/assets && cp -R vendor/dvsa/web-frontend-assets/dist/* public/assets/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "phpunit/phpunit-mock-objects": "2.3.*",
        "zendframework/zftool": "dev-master",
        "zendframework/zend-developer-tools": "dev-master",
        "firephp/firephp-core": "0.4.0",
        "whatthejeff/nyancat-phpunit-resultprinter": "~1.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://artifacts.mot.npm"
        },
        {
            "type": "git",
            "url": "git@gitlab.clb.npm:slot-purchase/web-frontend.git"
        },
        {
            "url": "git@gitlab.clb.npm:cpms/cpms-forms.git",
            "type": "git"
        },
        {
            "url": "git@gitlab.clb.npm:cpms/cpms-client.git",
            "type": "git"
        },
        {
            "type": "git",
            "url": "http://gitlab.clb.npm/mot/dvsa-application-logger.git"
        },
        {
            "type": "git",
            "url": "http://gitlab.clb.npm/sc/transitiongroup.git"
        }
    ],
    "config" : {
      "github-oauth" : {
        "github.com" : "c413da7d946413edba2adadd44beb7a5075a52aa"
      }
    }
}
