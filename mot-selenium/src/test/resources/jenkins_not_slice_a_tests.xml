<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Not Regression Tests" parallel="classes" thread-count="10">
    <test name="Not Regression test suite">
        <groups>
            <run>
                <exclude name="Regression"/>
            </run>
        </groups>
        <packages>
            <package name=".*"/>
        </packages>
    </test>
</suite>
