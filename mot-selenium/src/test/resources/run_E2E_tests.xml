<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Short Tests" parallel="classes" thread-count="1">
    <test name="E2E Tests">
        <groups>
            <run>
                <include name="E2E"/>
            </run>
        </groups>
        <packages>
            <package name=".*"/>
        </packages>
    </test>
</suite>
