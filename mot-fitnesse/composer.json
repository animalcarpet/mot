{
    "name": "dvsa/mot-fitnesse",
    "description": "Fitnesse test implementation for DVSA MOT modernisation",
    "require": {
        "zendframework/zendpdf": "2.0.2",
        "zendframework/zend-log": "2.3.*",
        "dvsa/scripts": "dev-master"
    },
    "repositories": [
       {
          "type": "composer",
          "url": "https://packages.motdev.org.uk"
       }
    ],
    "autoload": {
        "psr-0": {
            "": ["vendor/", "FitNesseRoot/Slim"],
            "DvsaCommon" : "../mot-common-web-module/src"
        },
        "classmap" : [
            "../mot-common-web-module/Module.php"
        ]
    },
    "scripts": {
      "post-install-cmd": [
        "cd .. && ln -fs mot-fitnesse/vendor/dvsa/scripts/jenkins Jenkins_Scripts"
      ]
    },
    "config" : {
      "github-oauth" : {
        "github.com" : "c413da7d946413edba2adadd44beb7a5075a52aa"
      }
    }
}
